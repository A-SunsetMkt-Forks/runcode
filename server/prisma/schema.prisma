generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model stat {
  id        Int      @id @default(autoincrement())
  ip        String
  country   String?  @default("")
  province  String?  @default("")
  city      String?  @default("")
  isp       String?  @default("")
  userAgent String?  @default("")
  createdAt DateTime @default(now())
}
